import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int s=sc.nextInt();
        int t=sc.nextInt();
        int r=s/t;
        int d=s%t;
        System.out.printf("The number of students in each team is %d and left out is %d",r,d);
    }
}
